      <section id="latest-events">
          <div class="container">
              <div class="row">
                {{#each results.latestEvents as |events i|}}
                      <div class="col-lg-4 col-md-4 col-sm-6">
                          <div class="event-feed latest">
                              <img src="img/{{events.eventThumbnail}}" alt="">
                              <div class="date">
                                  <span class="day">{{events.eventDay}}</span>
                                  <span class="month">{{events.eventMonth}}</span>
                              </div>
                              <h5><a href="event-detail.html">{{events.eventName}}</a></h5>
                              <ul>
                                  <li><b class=" fa fa-location-arrow"></b> at {{events.eventVenue}}</li>
                                  <li><b class=" fa fa-clock-o"></b> {{events.eventStartTime}} - {{events.eventStopTime}}</li>
                                  
                              </ul>
                              <a class="btn" href="{{events.eventUrl}}" target="_blank">View Event</a>
                          </div><!--\\event-feed latest-->
                      </div>
                {{/each}}   
              </div><!--row-->
          </div><!--//container-->  
      </section>
